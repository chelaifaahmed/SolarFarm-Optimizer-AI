# OpenAI API Key (for CrewAI and LangChain)
OPENAI_API_KEY=your_openai_api_key_here

# Groq API Key (alternative to OpenAI)
GROQ_API_KEY=your_groq_api_key_here

# Google Earth Engine (optional - for live satellite data)
GOOGLE_EARTH_ENGINE_PROJECT=your_gee_project_id

# OpenStreetMap API (no key required, but can set custom endpoint)
OSM_API_ENDPOINT=https://overpass-api.de/api/interpreter

# Model Configuration
USE_GROQ=False
MODEL_NAME=gpt-4-turbo-preview
TEMPERATURE=0.7

# Application Settings
MAX_ITERATIONS=100
POPULATION_SIZE=1000
GENETIC_GENERATIONS=50
PROCESSING_TIMEOUT=300

# Vision Model Paths (will be auto-downloaded if not present)
SAM_CHECKPOINT_PATH=./models/sam_vit_h_4b8939.pth
YOLO_MODEL_PATH=./models/yolov8x.pt
